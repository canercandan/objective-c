@import "Animal.kh"

@implementation Animal
{
  @member void init(int age, int poid, int faim)
    {
      [self.age] = age;
      [self.poid] = poid;
      [self.faim] = faim;
    }
  @member void	init()
    {
      [self.age] = 1;
      [self.poid] = 5;
      [self.faim] = 1;
    }
  @virtual @member void	crier()
    {
      printf("Je suis fouu..!");
    }
}

