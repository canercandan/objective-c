
implementation
	::= #implicitCopy [ addImplementation ]
;

addImplementation
	::= implementationPrefix #readIdentifier:name
	    => existModule(name);
	    => insert #evaluateVariable("this.kooc.block." + name);
	    '{' [ declaration(this.block.kooc.name) ]* '}'
;

implementationPrefix
	::= "@implementation"
;
