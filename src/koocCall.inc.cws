function	isAnInstance(key : value, type : node)
{
  	local leaf;

	ref leaf = type#parent;
	while ((leaf.variables[key].oname != key) && existVariable(leaf.pctx))
	  ref leaf = leaf.pctx;
	return leaf.variables[key].ctype.identifier;
}

function	getParent(child : node)
{
	return (child.parent);
}