#implicitCopy

#ignore["KOOC"] ::=
[['#' ->[['\r']? '\n']]
 | ["//" ->[['\r']? '\n']]
 | ["/*" ->"*/"]
 | ' ' | '\t' | '\n' | '\r' | '\f'
]+
;

#overload translation_unit ::=
#ignore("KOOC")
[koocDeclaration(this.block)
 | declaration(this.block)]*
#continue #empty
;

koocDeclaration(block : node) ::= [ import | module | implementation | appel_KOOC ];

