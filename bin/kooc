if [ $1 ]
then
    codeworker -nologo -stack 800000 -script \
	${KOOC_PATH:?You have to set KOOC_PATH}/src/kooc.cws \
	-args $*;
    if [ $2 ]
    then
	echo "==== BEGIN $2 ====";
	cat -e $2;
	echo "==== END $2 ====";
    fi
else
    echo "I need Koala";
fi
